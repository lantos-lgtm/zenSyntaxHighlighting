std: std()
{
     String,
     Boolean,
     Int,
     Type
}: std.types.String

// this is a comment
Person: Type {
     name: String
     age: Int.i32
}

person: Person(
     name: String("OpenAI"),
     age: Int.i32(42)
)

isOpenAi: Function {
     args: {
          person: Person
     },
     return: {Boolean},
     body: {
          if (person.name == String("OpenAI")) {
               return(Boolean(false))
          } else {
               return(Boolean(true))
          }
     }
}

io.print(isOpenAi(person))

